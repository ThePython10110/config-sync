{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "palette": {
    "green1": "#1B3900",
    "green2": "#1F4700",
    "green3": "#417200",
    "green4": "#428400",
    "green5": "#069200",
    "green6": "#5FB500"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "accordion",
          "powerline_symbol": "î‚°",
          "foreground": "#ffffff",
          "background": "p:green4",
          "properties": {
            "style": "full"
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "p:green3",
    "foreground": "#ffffff",
    "template": "{{ .Shell }}> "
  }
}
